<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
       
<mapper namespace="com.project.comic.notice.NoticeDAO">
   
   <insert id="addNotice" parameterType="com.project.comic.notice.NoticeDTO">
   insert into t_noticeboard(uidx,title,content,originimg,fakeimg,noname)
   values(#{uidx},#{title},#{content},#{originimg},#{fakeimg},#{noname})
   </insert>

</mapper>